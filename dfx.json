      "wasm": "https://download.dfinity.systems/ic/1db8f933fdadc81a90e7db2389b081e21263a9b6/canisters/ic-icrc1-ledger.wasm.gz",
      "declarations": {
        "output": "Frontend/src/declarations/icrc2_token",
        "bindings": ["js", "ts", "did"]
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}