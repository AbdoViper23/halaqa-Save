      "declarations": {
        "output": "Frontend/src/declarations/Backend",
        "bindings": ["js", "ts", "did"]
      }
    },
    "Frontend": {
      "source": ["Frontend/dist"],
      "type": "assets"
    },
    "icrc2_token": {
      "type": "custom",
